{
  "name": "@tkesgar/reine",
  "version": "0.2.0",
  "description": "Stale-while-revalidate caching utility library",
  "keywords": [
    "stale-while-revalidate",
    "cache",
    "library"
  ],
  "license": "MIT",
  "author": "Ted Kesgar <t.kesgar@hey.com> (https://tkesgar.com)",
  "contributors": [
    "Ted Kesgar <t.kesgar@hey.com> (https://tkesgar.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkesgar/reine.git"
  },
  "bugs": {
    "url": "https://github.com/tkesgar/reine/issues"
  },
  "homepage": "https://github.com/tkesgar/reine#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist/**"
  ],
  "main": "./dist/cjs/index.js",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "watch": "tsc -w",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --write '**/*'",
    "test": "jest",
    "clean": "rm -rf dist/**",
    "build:cjs": "tsc --build tsconfig-cjs.json",
    "build:mjs": "tsc --build tsconfig-mjs.json",
    "build": "npm run build:cjs && npm run build:mjs",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@tkesgar/reno": "^1.0.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "conventional-changelog-cli": "^2.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "prettier": "~2.2.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.3"
  }
}
